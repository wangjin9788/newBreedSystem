<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.breed.govern.mapper.BFermentDetailMapper">
<select id="getFermentDetailList" resultType="com.breed.govern.entity.BFermentDetail">
    SELECT
        *
    FROM
        b_ferment_detail
    WHERE
        ff_id = #{id}

</select>
</mapper>
